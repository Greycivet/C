#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>

#define Num 10

enum Sex {
	Male,
	Female,
	Secret
};
int main() {
	//字面常量
	10;
	3.1415;

	//const-常属性
	//const修饰的常变量,后面不能修改,但本质上还是常量
	const int num = 6;
	//int arr[num] = {0}; //报错,数组长度需要放入常量,num属于常变量,本质上还是变量

	// #define定义的标识符常量 
	int arr[Num] = {0};

	//枚举常量
	//关键字:enum
	//每一个枚举常量都有一个值
	enum Sex s = Female;	//枚举常量是不能改的,枚举类型所创建的变量是可以改的
	printf("%d\n",s);
	printf("%d\n", Male);
	printf("%d\n", Secret);

	return 0;
}
