#include <stdio.h>


int num2 = 20;//全局变量
int main() {
	//全局变量和局部变量名字可以相同,使用时局部变量优先,建议不要相同


	int num1 = 10;//局部变量


	printf("HelloWorld!\n");

	char c = 'A';//创建一个char字符类型变量,赋值A

	printf("%c\n",c); //打印输出字符串格式数据

	int age = 20;

	printf("%d\n",age);//打印输出整型数据

	printf("%d\n", sizeof(char)); //打印输出数据类型长度
	printf("%d\n", sizeof(short));
	printf("%d\n", sizeof(int));
	printf("%d\n", sizeof(long));
	printf("%d\n", sizeof(long long));
	printf("%d\n", sizeof(float));
	printf("%d\n", sizeof(double));

	

	return 0;
}
